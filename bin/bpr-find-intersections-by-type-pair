#!/usr/local/bin/swipl -q -t main -s

:- use_module(bioprolog(gff)).
:- use_module(bioprolog(range)).

main:-
        current_prolog_flag(argv, Argv),
        writeln(argv=Argv),
        append(_, [--,T1,T2,F], Argv),
        load_gff(F),
        setof(ID1/ID2,
              (
                ffetch(ID1,[type=T1,seq=S,range=R1]),
                ffetch(ID2,[type=T2,seq=S,range=R2]),
                intersects(R1,R2),
                write(ID1),write(' '),write(ID2),nl
              ),_).
        

